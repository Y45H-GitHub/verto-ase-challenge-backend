    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.0",
        "@types/jest": "^29.5.8",
        "@types/supertest": "^2.0.16",
        "typescript": "^5.3.2",
        "ts-node-dev": "^2.0.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
